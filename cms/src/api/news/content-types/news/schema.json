{
  "kind": "collectionType",
  "collectionName": "news",
  "info": {
    "singularName": "news",
    "pluralName": "news",
    "displayName": "News",
    "description": "Actualit√©s et articles"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 255
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "excerpt": {
      "type": "string",
      "maxLength": 500
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::instructor.instructor"
    },
    "category": {
      "type": "string",
      "maxLength": 100
    },
    "tags": {
      "type": "json"
    },
    "featuredImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "publishedAt": {
      "type": "datetime"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    }
  }
}
